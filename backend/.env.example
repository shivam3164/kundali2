# Environment variables for Kundali API

# Application
APP_NAME="Kundali API"
APP_VERSION="1.0.0"
DEBUG=true
ENVIRONMENT=development

# API
API_PREFIX=/api/v1
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Database
DATABASE_URL=sqlite:///./kundali.db

# Ephemeris
# EPHEMERIS_PATH=/path/to/ephemeris/files
DEFAULT_AYANAMSA=lahiri

# Logging
LOG_LEVEL=INFO
